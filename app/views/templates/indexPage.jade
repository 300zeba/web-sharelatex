extends ../layout

block content

	.container
			.row
				#searchArea.span12.ng-scope(ng-app='templatesSearchApp')
					.ng-scope(ng-controller='SearchController')
						.row-fluid
							.span6.offset3.search-field
								i.icon-search
								input#searchField.span12.ng-pristine.ng-valid(type='text', ng-model='searchQueryText', ng-keyup='search()', placeholder='Search')
						ul.unstyled
							// ngRepeat: hit in hits
			.row
				.page-header.span12
					h2
						a(href='/templates/bibliographies') Bibliographies
				.span12
					.row-fluid
						.span3
							a(href='/templates/bibliographies/notes2bib---integrating-notes-into-the-bibliography')
								.template-thumbnail.thumbnail
									img(src='https://d1hk1zhcb5hn6i.cloudfront.net/533619d910f91d4e6b15f6e8/v/0/pdf-converted-cache/style-thumbnail')
									h3.txt-middle notes2bib - Integrating notes into the bibliography
