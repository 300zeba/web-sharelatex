									span.at_a11y
										i.fa.fa-twitter-square.fa-2x &nbsp;
							a.addthis_button_google_plusone_share.at300b(href='https://www.addthis.com/bookmark.php?v=300&winname=addthis&pub=ra-517c16586439faa7&source=tbx32-300&lng=en-US&s=google_plusone_share&url=https%3A%2F%2Fwww.sharelatex.com%2Ftemplates%2F52fb89a333621a613683ec95&title=thebibliography%20-%20LaTeX%20Template&ate=AT-ra-517c16586439faa7/-/-/53579a0bbe5257f9/2&frommenu=1&uid=53579a0bbceeb2e2&ct=1&pre=https%3A%2F%2Fwww.sharelatex.com%2Ftemplates%2F&tt=0&captcha_provider=nucaptcha', target='_blank', title='Google+')
