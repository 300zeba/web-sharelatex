extends ../layout

block content
	.container
		.page-header
				h2
					a(href='/templates') Templates
					|  ›
					a(href='/templates/bibliographies')  Bibliographies
					|  › thebibliography
		.container.txt-lefty
			.row-fluid
				.span6
					a(href='https://d1hk1zhcb5hn6i.cloudfront.net/52fb89a333621a613683ec95/v/1/pdf')
						img.sample-template(src='https://d1hk1zhcb5hn6i.cloudfront.net/52fb89a333621a613683ec95/v/1/pdf-converted-cache/style-preview')
				.span6.ng-scope(ng-app='openInSl')
					h3 About
					div
						p This document is an example of thebibliography using in bibliography management.
					span.ng-scope(ng-controller='openInSlController')
						p
							a.btn.btn-primary.btn-large.open-in-sharelatex.ng-binding(href='/project/new/template?zipUrl=%2Ftemplates%2F52fb89a333621a613683ec95%2Fv%2F1%2Fzip&templateName=thebibliography&compiler=pdflatex', ng-click='open()', ng-disabled='isDisabled', rel='nofollow') Open in ShareLaTeX
							| &nbsp;
							a.btn.btn-large.download-zip(href='/templates/52fb89a333621a613683ec95/v/1/zip', rel='nofollow', ng-click='downloadZip()', data-toggle="tooltip", data-placement="bottom", title="", data-original-title="Download .zip File")
								i.fa.fa-cloud-download
					.social_buttons
						.addthis_toolbox.addthis_default_style.addthis_32x32_style
							a.addthis_button_facebook.at300b(title='Facebook', href='#')
								span.at300bs.at15nc.at15t_facebook
									span.at_a11y
										i.fa.fa-facebook-square.fa-2x &nbsp;
							a.addthis_button_twitter.at300b(title='Tweet', href='#')
